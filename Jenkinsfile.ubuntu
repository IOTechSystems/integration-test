pipeline {
    agent any
    triggers { cron('0 6 * * *') }
    node("ubuntu1604-vm1"){

        stages{
            stage("integration-test"){
                script {

                    checkout scm

                    def rootDir = pwd()
                    def integrationTestJob = load "${rootDir}/integrationTestJob.groovy"

                    integrationTestJob.runNode()
                }
            }
        }

    }

}

